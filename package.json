{
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss-cli": "^11.0.1",
    "rtlcss": "^4.3.0",
    "sass": "^1.93.2",
    "vite": "^7.1.9",
    "vite-plugin-sass-dts": "^1.3.31"
  },
  "scripts": {
    "build": "vite build && npm run bootstrap:build",
    "start": "vite",
    "bump:patch": "npm version patch",
    "bump:minor": "npm version minor",
    "bump:major": "npm version major",
    "package:zip": "zip -r wp2-update.zip . -x \"node_modules/*\" \"tests/*\" \"*.git/*\" \"*.vscode/*\" \"*.DS_Store\" \"*.env\" \"*.log\"",
    "bootstrap:build": "SASS_PATH=node_modules sass --quiet --quiet-deps assets/styles/admin-main.scss dist/wp2-admin-main.css --no-source-map && postcss dist/wp2-admin-main.css --use autoprefixer -o dist/wp2-admin-main.css && rtlcss dist/wp2-admin-main.css dist/wp2-admin-main-rtl.css"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.5",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "lodash": "^4.17.21",
    "nanostores": "^0.10.3",
    "semver": "^7.7.3",
    "tabbyjs": "^12.0.3",
    "toastify-js": "^1.12.0"
  }
}